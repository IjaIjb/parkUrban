{
  "name": "urban",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev --turbo\" \"tailwindcss -i styles/globals.css -o styles/dist.css -w\"",
    "build": "tailwindcss -m -i src/app/globals.css -o src/app/dist.css && next build && next build",
    "https": "next dev --experimental-https",
    "start": "next start",
    "lint": "next lint",
    "slow": "next dev",
    "test": "npx playwright test",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@heroicons/react": "^1.0.6",
    "@mui/icons-material": "^5.14.3",
    "@mui/material": "^5.14.5",
    "@reduxjs/toolkit": "^1.9.5",
    "@types/node": "18.15.11",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@vercel/analytics": "^1.0.2",
    "@vercel/speed-insights": "^1.0.11",
    "axios": "^1.4.0",
    "clsx": "^2.1.1",
    "concurrently": "^8.2.0",
    "crypto": "^1.0.1",
    "crypto-js": "^4.1.1",
    "cryptr": "^6.3.0",
    "dexie": "^4.0.4",
    "dexie-react-hooks": "^1.1.7",
    "eslint": "8.37.0",
    "eslint-config-next": "^14.2.3",
    "firebase": "^9.21.0",
    "formik": "^2.2.9",
    "i": "^0.3.7",
    "jwt-decode": "^3.1.2",
    "ms": "^2.1.3",
    "next": "^14.2.3",
    "next-nprogress-bar": "^2.1.0",
    "nextjs-progressbar": "^0.0.16",
    "nookies": "^2.5.2",
    "npm": "^10.7.0",
    "prettier": "^2.8.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^4.8.0",
    "react-nextjs-toast": "^1.2.5",
    "react-paystack": "^4.0.3",
    "react-redux": "^8.1.2",
    "react-select-country-list": "^2.2.3",
    "react-spinners": "^0.13.8",
    "react-svg": "^16.1.7",
    "react-tabs": "^6.0.0",
    "react-toastify": "^9.1.3",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "sass": "^1.66.1",
    "tailwind-merge": "^2.3.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.2",
    "@types/react-select-country-list": "^2.2.1",
    "@types/redux-logger": "^3.0.9",
    "autoprefixer": "^10.4.14",
    "encoding": "^0.1.13",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.3.1",
    "@playwright/test": "^1.44.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.4",
    "playwright": "^1.44.0",
    "typescript": "^5.4.5"
  }
